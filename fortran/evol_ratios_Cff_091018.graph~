set term post col enhanced 14
set out 'evol_ratios_Cff_091018.eps'
set title 'TMD gluon-gluon initiated C[f_1^gf_1^g] values for Q=8,12,21GeV, x_1=x_2=10^{-2} and various NP Sudakovs'
set xlabel 'q_T (GeV^{-1})'
set ylabel 'C[f_1^gf_1^g]'

BLNY8='ratiodata/ratios_Q8.0_SnpBLNY_091018.dat'
BLNY12='ratiodata/ratios_Q12.0_SnpBLNY_091018.dat'
BLNY21='ratiodata/ratios_Q21.0_SnpBLNY_091018.dat'
KN8='ratiodata/ratios_Q8.0_SnpKN_091018.dat'
KN12='ratiodata/ratios_Q12.0_SnpKN_091018.dat'
KN21='ratiodata/ratios_Q21.0_SnpKN_091018.dat'
AR8='ratiodata/ratios_Q8.0_SnpAR_091018.dat'
AR12='ratiodata/ratios_Q12.0_SnpAR_091018.dat'
AR21='ratiodata/ratios_Q21.0_SnpAR_091018.dat'
CR8='ratiodata/ratios_Q8.0_SnpCRsimp_091018.dat'
CR12='ratiodata/ratios_Q12.0_SnpCRsimp_091018.dat'
CR21='ratiodata/ratios_Q21.0_SnpCRsimp_091018.dat'

plot \
BLNY8 u (($1)):(($2)) title 'BLNY Q=8' w l lw 3 lt 1 lc rgb "red",\
BLNY12 u (($1)):(($2)) title 'BLNY Q=12' w l lw 3 lt 2 lc rgb "red",\
BLNY21 u (($1)):(($2)) title 'BLNY Q=21' w l lw 3 lt 3 lc rgb "red",\
KN8 u (($1)):(($2)) title 'KN Q=8' w l lw 3 lt 1 lc rgb "green",\
KN12 u (($1)):(($2)) title 'KN Q=12' w l lw 3 lt 2 lc rgb "green",\
KN21 u (($1)):(($2)) title 'KN Q=21' w l lw 3 lt 3 lc rgb "green",\
AR8 u (($1)):(($2)) title 'AR Q=8' w l lw 3 lt 1 lc rgb "blue",\
AR12 u (($1)):(($2)) title 'AR Q=12' w l lw 3 lt 2 lc rgb "blue",\
AR21 u (($1)):(($2)) title 'AR Q=21' w l lw 3 lt 3 lc rgb "blue",\
CR8 u (($1)):(($2)) title 'CR Q=8' w l lw 3 lt 1 lc rgb "black",\
CR12 u (($1)):(($2)) title 'CR Q=12' w l lw 3 lt 2 lc rgb "black",\
CR21 u (($1)):(($2)) title 'CR Q=21' w l lw 3 lt 3 lc rgb "black",\
