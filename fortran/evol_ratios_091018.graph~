set term post col enhanced 14
set out 'norm_distr_cp.eps'
set title 'Gener of gaussian distrib of MSTW2008 PDF values at x=10^{-2} and mu=mubfrozen(bt) for bt = 0 to 2 GeV^{-1}'
set xlabel 'gMSTW(x,mu)'
set ylabel 'Effective for sample size Nsamp=100000'
data1='norm_distr_data_cp0.0.dat'
data2='norm_distr_data_cp0.5.dat'
data3='norm_distr_data_cp1.0.dat'
data4='norm_distr_data_cp1.5.dat'
data5='norm_distr_data_cp2.0.dat'
plot data1 title 'bt=0GeV^{-1}',data2 title 'bt=0.5GeV^{-1}',data3 title 'bt=1GeV^{-1}',data4 title 'bt=1.5GeV^{-1}',data5 title 'bt=2GeV^{-1}'
